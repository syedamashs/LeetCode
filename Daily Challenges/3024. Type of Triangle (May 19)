class Solution {
    public String triangleType(int[] nums) {
        int i=0;

        if(nums[i]+nums[i+1]>nums[i+2] && nums[i]+nums[i+2]>nums[i+1] && nums[i+1]+nums[i+2]>nums[i]){

        if(nums[i]==nums[i+1] && nums[i]==nums[i+2]){
            return "equilateral";
        }

        if(nums[i]==nums[i+1] || nums[i]==nums[i+2] || nums[i+1]==nums[i+2]){
            return "isosceles";
        }

        if(nums[i]!=nums[i+1] && nums[i+1]!=nums[i+2] &&  nums[i+1]!=nums[i+2]){
                    return "scalene";}
        }
        return "none";
        }
    }
