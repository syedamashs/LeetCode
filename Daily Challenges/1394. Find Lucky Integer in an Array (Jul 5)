class Solution {
    public int findLucky(int[] arr) {
        
        HashMap<Integer,Integer> map = new HashMap<>();
        for(int num : arr){
            map.put(num,map.getOrDefault(num,0)+1);
        }

        int maxk=-1;

        for(Map.Entry<Integer,Integer> entry: map.entrySet()){
            if(entry.getValue()==entry.getKey()){
                maxk = entry.getKey();
            }
        }

        return maxk;
    }
}
